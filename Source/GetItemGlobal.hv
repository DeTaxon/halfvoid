GetItemGlobal2 := !(Vector.{Str} names, !(Vector.{BoxUnit^}, Vector.{Str})&->bool cb) -> void 
{
	
	lst := MakeVectorOnStack.{BoxUnit^,10}()
	resNames := MakeVectorOnStack.{Str,10}()

	for spc : CodeSpaces.Reverse
	{
		lst.Destroy()
		resNames.Destroy()

		for nm : names
		{
			if spc.Globals.Contain(nm)
			{
				lst.Push(spc.Globals[nm][^])

				while resNames.Size() <	lst.Size()
					resNames.Push(nm)
			}
			if spc.Functions.Contain(nm)
			{
				lst.Push(spc.Functions[nm][^])
				while resNames.Size() <	lst.Size()
					resNames.Push(nm)
			}
			if spc.Templates.Contain(nm)
			{
				lst.Push(spc.Templates[nm][^])
				while resNames.Size() <	lst.Size()
					resNames.Push(nm)
			}
		}
		if lst.Size() != 0
		{
			if cb(lst,resNames)
				return void
		}
	}
}
