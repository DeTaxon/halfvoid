
GetItemGlobal := !(Str name, !(Vector.{BoxUnit^})&->bool cb) -> void
{
	lst := MakeVectorOnStack.{BoxUnit^,10}()
	for spc : CodeSpaces.Reverse
	{
		lst.Destroy()
		if spc.Globals.Contain(name)
		{
			lst.Push(spc.Globals[name][^])
		}
		if spc.Functions.Contain(name)
		{
			lst.Push(spc.Functions[name][^])
		}
		if spc.Templates.Contain(name)
		{
			lst.Push(spc.Templates[name][^])
		}
		if lst.Size() != 0
		{
			if cb(lst)
				return void
		}
	}
	for spc : CodeSpaces.Reverse
	{
		lst.Destroy()
		for cl : spc.Modules
		{
			fnc := cl.GetItem(name)
			if fnc != null
				lst.Push(fnc)
		}
		if lst.Size() != 0
		{
			if cb(lst)
				return void
		}
	}
}

