
MCInterpContext := class
{

	declarePtrs := AVLMap.{void^,void^}

	DeclareToPtr := !(BoxFuncDeclare^ dclr) -> void^
	{
		return declarePtrs[dclr]?

		fName := dclr.GetName()

		switch(fName) //TODO load them from libraries
		{
		case "puts"
			result = puts
		case "printf"
			result = printf
		case "malloc"
			result = malloc
		case "free"
			result = free
		case void
			assert(false)
		}

		declarePtrs[dclr] = result
	}
}

