
GetItemLocal := !(Str name, !(Vector.{BoxUnit^})&->bool cb) -> void
{
	lst := MakeVectorOnStack.{BoxUnit^,20}()

	for it : UnitStack
	{
		//if it is in NamedVar
		//{
		//	if it->{NamedVar^}.Name == name
		//	{
		//		lst.Push(it)
		//	}
		//}
		if it is ConstVar
		{
			cn := it->{ConstVar^}
			if cn.Name == name
			{
				lst.Push(cn)
			}
		}
		if it is AliasVar
		{
			cn := it->{AliasVar^}
			if cn.itName == name
			{
				lst.Push(cn)
			}
		}
	}
	if lst.Size() != 0
	{
		cb(lst)
	}
}
