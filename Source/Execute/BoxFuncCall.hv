AppendClass BoxFuncCall
{
	IsExecutable := virtual true
	Execute := virtual !(ExecutionContext^ ctx,BoxExeObj^[] args) -> BoxExeObj^
	{
		thisArgs := HybridQueue.{BoxExeObj^,10}

		for it : downs
		{
			thisArgs.Push(it.Execute(ctx,args))
		}
		if callObj is BoxFuncDeclare
		{
			dclr := callObj->{BoxFuncDeclare^}
			if dclr.itName == "puts"
			{
				puts(thisArgs[0]->{BoxExeString^}.val.NonNullStr())
			}
		}

	}
}
