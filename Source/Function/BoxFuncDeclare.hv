BoxFuncDeclare := class extend BoxFuncCommon
{
	itName := StringSpan
	this := !(TypeFunc^ tp, StringSpan fName) -> void
	{
		SetType(tp)
		itName = fName
	}

	PrintCode := virtual !(Stream^ f) -> void
	{
		f^ << "declare "
		PrintResultType(f)
		f^ << " @" << itName << "("
		PrintInputTypes(f)
		f^ << ")\n"
	}
	PrintName := virtual !(Stream^ f) -> void
	{
		f^ << "@" << itName
	}
}
