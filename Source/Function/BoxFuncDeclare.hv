BoxFuncDeclare := class extend BoxFuncCommon
{
	itName := StringSpan
	this := !(TypeFunc^ tp, StringSpan fName) -> void
	{
		SetType(tp)
		itName = fName
	}

	PrintCode := virtual !(LLVMWriteContext^ ctx) -> void
	{
		f := ctx.writeStream

		f^ << "declare "
		PrintResultType(ctx)
		f^ << " @" << itName << "("
		PrintInputTypes(ctx)
		f^ << ")\n"
	}
	PrintName := virtual !(LLVMWriteContext^ ctx) -> void
	{
		ctx.writeStream^ << "@" << itName
	}
}
