StripMem := class extend BoxExeObj
{
	callObj := BoxExeObj^
	this := !(BoxExeObj^ p) -> void
	{
		callObj = p

		ObjType = callObj.GetType()
	}
	IsMem := virtual !() -> bool { return false }

	PrintPre := virtual !(LLVMWriteContext^ ctx) -> void { callObj.PrintPre(ctx) }
	PrintUse := virtual !(LLVMWriteContext^ ctx) -> void { callObj.PrintUse(ctx) }

	PrintDebugMeta := virtual !(LLVMWriteContext^ ctx) -> void
	{
		callObj.PrintDebugMeta(ctx)
	}
}
