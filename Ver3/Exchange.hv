IntExchange := class extend BoxExeObj
{
	resId := int
	Down := BoxExeObj^
	this := !(BoxExeObj^ obj, Type^ toType) -> void
	{
		resId = GetNewId()
		Down = obj
		ObjType = toType
	}
	IsMem := virtual !() -> bool { return false }
	PrintPre := virtual !(TIOStream^ f) -> void
	{
		inType := Down.GetType()
		Down.PrintPre(f)
		f^ << "%T" << resId << " = "
		if inType.GetSize() > ObjType.GetSize()
		{
			f^ << "trunc"
		}else{
			if IsUInt(inType)
			{
				f^ << "zext"
			}else{
				f^ << "sext"
			}
		}
		f^ << " "
		inType.PrintType(f)
		f^ << " "
		Down.PrintUse(f)
		f^ << " to "
		ObjType.PrintType(f)
		if DebugMode
			f^ << ", !dbg !" << GDebugLineCallId
		f^ << "\n"

	}
	PrintUse := virtual !(TIOStream^ f) -> void
	{
		f^ << "%T" << resId
	}
}

