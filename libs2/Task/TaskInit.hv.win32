
TaskOSInit := !() -> void
{
	TaskMainEvent = new Event()

	TaskPollSize = 1
	TaskPollList[0] = TaskMainEvent.Get()
	TaskPollFunctions[0] = TaskMainEventCB
}
TaskOSDestroy := !() -> void
{
	TaskMainEvent = null
}


TaskMainEvent := Event^
TaskMainEventCB := !() -> void
{
	
}

TaskPollSize := int
TaskPollFunctions := void^[32]
TaskPollList := void^[32]
