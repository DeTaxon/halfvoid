posixGetFileSize := !(int fd,u64^ size) -> bool
{
	dataD := char[144]
	val := fstat(fd,dataD->{char^})
	if val == -1 return false
	asNeed1 := dataD->{char^}[48]&
	asNeed2 := asNeed1->{u64^}
	size^ = asNeed2^
	return true
}
