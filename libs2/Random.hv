
initedRandom := bool
initRandom := !() -> void
{
	if initedRandom
		return void
	initedRandom = true
	x := time(null)
	srand(x&->{int^}^)
}

RandInt := !(Range rRange) -> int
{
	//TODO if function is used, call initRandom at start
	initRandom()

	res := rand()

	dif := rRange.end - rRange.start
	
	res = res mod dif
	return res + rRange.start
}
