MultilineText := class extend GUIObject
{
	lines := Str[]
	this := !() -> void
	{
		SetSize("static",100,"static",100)
	}
	this := !(Str[] newLines) -> void
	{
		this."this"()
		AddLinesLink(newLines)
	}
	AddLinesLink := !(Str[] newLines) -> void
	{
		lines = newLines
	}
	Refresh := !() -> void
	{
		
	}
	DrawCli := virtual !(DrawCliContext^ ctx) -> void
	{
		for it,i : lines
		{
			mvaddstr(i+1,1,it)
		}
	}
}
