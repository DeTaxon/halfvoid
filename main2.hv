
main := !(char^[] args) -> int
{
	test := LexBuilder
	test.ApplyReg("[a-zA-Z\\-\\.]+")
	m := test.GenerateMachine()
	m.PrintIt()

	gRepo := new vRepo
	gRepo.Init(".")

	tst := gRepo.GetFile("main2.hv")
	mp := tst.GetMap()

	m.RegExpReadText(mp.Get(),mp.Size(), (st,a) ==> {
		printf("hm <%s>\n",st.Normalize().Str())
	})
	
	return 0
}
