
mini_class := class
{
	x := !()&->int 
	this := !(i) -> void
	{
		x = i
	}
	"~For" := !() -> !()&->int
	{
		return x
	}
}

main := !(int argc, char^^ argv) -> int
{
	t := () ==> int
	{
		i := 0
		while i < 3
		{
			yield i
			i += 1
		}
	}

	tmp := mini_class(t)

	for i : tmp
	{
		printf("heh %i\n",i)
	}


	return 0
}

