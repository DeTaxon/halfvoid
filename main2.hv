j := int
k := 1000000
mtx := Mutex^
main := !(char^[] args) -> int
{	
	mtx := new Mutex()

	x := new Thread(() ==> {
		for i : k
		{
			mtx.Lock()
			j += 1
			mtx.Unlock()
		}
		printf("second\n")
	})
	for i : k
	{
		mtx.Lock()
		j += 1
		mtx.Unlock()
	}
	x.Join()
	printf("res %i\n",j)
	return 0
}
