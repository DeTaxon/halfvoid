
MixinObject1 := class
{
	Contain := poison !(!(int)&-> bool l) -> bool
	{
		if l(this[^])
			return true
		return false
	}
}
MixinObject2 := class
{
	Test := poison !() -> bool
	{
		printf("hello\n")
	}
}

TestClass := class extend MixinObject1 , MixinObject2
{
	"~For" := !() -> !()&->int
	{
		return () ==>
		{
			for it : ![1,12,100,-3,16]
				yield it			
		}
	}
}


main := !(int argc, char^^ argv) -> int
{
	c := TestClass
	printf("wow 12 %i\n",c.Contain(_1 == 12))
	printf("wow 11 %i\n",c.Contain(_1 == 11))
	c.Test()
	return 0
}

