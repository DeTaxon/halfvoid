"@try_all block" := !(func,num) -> void
{	
	printf("heh %i\n",num)
	for i : num
	{
		try
		{
			func(i)
		}catch(IException^ e)
		{
			printf("here\n")
		}
	}
}
"@try_one block" := !(func,num) -> void //TODO #uniq
{	
	printf("heh %i\n",num)
	for i : num
	{
		try
		{
			func(i)
			return void
		}catch(IException^ e)
		{
			printf("here\n")
		}
	}
}

"@twice for" := !(func) -> void
{
	func((x) ==> { 
		x() 
		x() 
	})
}

