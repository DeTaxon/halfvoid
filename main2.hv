pfunc := !(void^ v, char^ data, ...) -> void
{
	lst := char[80]
	va_start(lst)
	vprintf(data,lst)
}



main := !(int argc, char^^ argv) -> int
{
	squirrel_init()
	tst := sq_open(1024)
	sq_pushroottable(tst)
	sq_setprintfunc(tst,pfunc,pfunc)
	sqstd_seterrorhandlers(tst)
	res := sqstd_dofile(tst,"test.nut",0,1)
	sq_close(tst)
	return 0
}
