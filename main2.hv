
"@temp2 call" := !(@LType lambd) -> LType->ResultType
{
	TempPoolBlock()
	if result->Type->Group == "Class"
	{
		_SetAllocator(lambd(),gTemporaryPool&)
	} else if result->Type->Group == "Pointer"
	{
		_SetAllocator(lambd()^,gTemporaryPool&)
	}else{
		lambd()
	}
}

