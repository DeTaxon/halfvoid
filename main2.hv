Cls := class
{
	x := int
	y := s64
	z := int

	check := virtual !() -> void
	{
		printf("nope %i\n",x)
	}
}
Cls2 := class extend Cls
{
	w := int
	check := virtual !() -> void
	{
		printf("yep %i\n",x)
	}
}
internalInitClass := !(@T^ x)  -> void
{
	x->VTable = T->VTable
}
main := !(int argc, char^^ argv) -> int
{
	y := Cls2
	internalInitClass(y&)
	y.x = 1
	y.y = 2
	y.z = 3
	i := Cls^
	i = y&
	i.check()
}
