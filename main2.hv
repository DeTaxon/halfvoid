
c := !() -> void
{
	defer printf("def 1\n")
	throw new Exception
}

main := !(char^[] args) -> int
{
	libGlfwInit()

	glfwInit()

	win := glfwCreateWindow(500,500,"hi",null,null)

	glfwMakeContextCurrent(win)

	while not glfwWindowShouldClose(win)
	{
		glfwSwapBuffers(win)
		glfwPollEvents()
	}

	glfwTerminate()
	
	return 0
}

