TestWave := !() -> void
	#OptimizationWave()
{
	fnc := HV.FindFunction("OptTest")

	c := HV_Object^

	HV.ForEachFuncElement(fnc,(x) ==> {
		printf("hoy %s %i\n",x.ElementName,x.SubElementsCount())
		if x.ElementName == "FunctionCall"
		{
			c = x
		}
		if x.ElementName == "String"
		{
			printf("str value %s\n",x.GetStringValue())
			newStr := HV.CreateString("HAHA\n")
			c.ReplaceSubElement(0,newStr)
		}
	})

	fncCount := 0
	HV.ForEachFunction(x ==> {
		fncCount += 1
	})
	printf("func count %i\n",fncCount)
}

