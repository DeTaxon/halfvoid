
"for" := !(int this) -> RangeIterator
{
	result.end = this
}

RangeIterator := class //WARNING end value not included
{
	value := int
	end := int
	"()" := !() -> int
	{
		result = value
		value += 1
	}
	IsFinished := !() -> bool
	{
		return value > end
	}
}

Range := class
{
	start := int
	end := int

	this := !(int s, int e) -> Range
	{
		start = s
		end = e
	}
	"for" := !() -> RangeIterator
	{
		result.value = start
		result.end = end + 1
	}
	"in" := !(int x) -> bool
	{
		result = x >= start and x <= end
	}
}
".." := !(int s, int e) -> Range
{
	result.start = s
	result.end = e
}
