
stbtt_pack_context := class
{
	user_allocator_context := void^
	pack_info := void^
	width := int
	height := int
	stride_in_bytes := int
	padding := int
	skip_missing := int
	h_oversample := u32
	v_oversample := u32
	pixels := void^
	nodes := void^
}

stbtt_aligned_quad := class
{
	x0 := float
	y0 := float
	s0 := float
	t0 := float
	x1 := float
	y1 := float
	s1 := float
	t1 := float
}

stbtt_packedchar := class
{
	x0 := u16
	y0 := u16
	x1 := u16
	y1 := u16
	xoff := float
	yoff := float
	xadvance := float
	xoff2 := float
	yoff2 := float
}

stb_pack_range := class
{
	font_size := float
	first_unicod_codepoint_in_range := int
	array_of_unicode_codepoints := int^
	num_chars := int
	chardata_for_range := stbtt_packedchar^
	h_oversample := u32
	v_oversample := u32
}

