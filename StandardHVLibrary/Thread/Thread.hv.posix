AppendClass Thread
{
	itData := u64
	threadStart := !() -> void
	{
		pthread_create(itData&,null,_threadProxyFunc,this&->{void^})
	}
	Join := !() -> void
	{
		pthread_join(itData,null)
	}
}

BasicThread := class
{
	itData := u64
	Start := !(void^ fnc,void^ data) -> void
	{
		pthread_create(itData&,null,fnc,data)
	}
	Join := !() -> void
	{
		pthread_join(itData,null)
	}
}
