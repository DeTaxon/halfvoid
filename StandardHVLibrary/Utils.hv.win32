
GetCpuCount := !() -> int
{
	win32InitGetSystemInfo()
	return win32SysInfo.dwNumberOfProcessors
}
GetPageSize() := !() -> int
{
	win32InitGetSystemInfo()
	return win32SysInfo.dwPageSize
}



win32SysInfo := SYSTEM_INFO
win32InitGetSystemInfo := !() -> void
{
	first := static true
	
	if first
	{
		first = false
		GetSystemInfo(win32SysInfo&)
	}
}

