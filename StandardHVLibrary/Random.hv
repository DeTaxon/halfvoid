
initRandom := !() -> void
{
	x := time(null)
	srand(x&->{int^}^)
}


#MarkFunction("JITPass",RandInt)
RandInt := !(Range rRange) -> int
{
	#MarkFunction("CallAtStart",initRandom)

	res := rand()

	dif := rRange.End - rRange.Start
	
	res = res mod dif
	return res + rRange.Start
}

RandFloat := !(float a, float b) -> float
	#Mark("JITPass")
{
	#MarkFunction("CallAtStart",initRandom)

	mInt := 65536
	proc := RandInt(0..mInt) / mInt

	return a + (b - a)*proc
}
